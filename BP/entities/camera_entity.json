{
  "format_version": "1.17.0",
  "minecraft:entity": {
    "description": {
      "identifier": "fb:camera_entity_testing",
      "is_spawnable": false,
      "is_summonable": true,
      "is_experimental": false
    },
    "component_groups": {
      "fd:new_hitbox": {
        "minecraft:collision_box": {
          "width": 1,
          "height": 1
        }
      },
      "fd:rideable": {
      },
      "fd:despawn": {
        "minecraft:despawn": {},
        "minecraft:instant_despawn": {}
      },
      "fd:spawn": {
        "minecraft:collision_box": {
          "width": 1,
          "height": 1
        }
      }
    },
    "components": {
      "minecraft:type_family": {
        "family": [
          "horse"
        ]
      },
      "minecraft:knockback_resistance": {
        "value": 1
      },
      "minecraft:damage_sensor": {
        "triggers": [
          {
            "on_damage": {},
            "deals_damage": false
          }
        ]
      },
      "minecraft:nameable": {},
      "minecraft:health": {
        "min": 1,
        "value": 1
      }
    },
    "events": {
      "fd:despawn": {
        "run_command": {
          "command": [
            "setblock ~ ~ ~ air",
            "structure load camera_placer_loot ~ ~ ~"
          ]
        },
        "add": {
          "component_groups": ["fd:despawn"]
        }
      },
      "fd:despawn2": {
        "add": {
          "component_groups": ["fd:despawn"]
        }
      },
      "minecraft:entity_spawned": {
        "add": {
          "component_groups": ["fd:spawn"]
        }
      },
      "fd:remove_interact": {
        "remove": {
          "component_groups": ["fd:spawn"]
        },
        "add": {
          "component_groups": [
            "fd:new_hitbox"
          ]
        }
      },
      "ride_fd": {
        "add": {
          "component_groups": ["fd:rideable"]
        }
      },
      "no_ride_fd": {
        "remove": {
          "component_groups": ["fd:rideable"]
        }
      }
    }
  }
}
