				"flame_odds": 30,
				"burn_odds": 30
			},
			"minecraft:geometry": "geometry.stage_floor",
			"minecraft:aim_collision": {
				"origin": [
					-8,
					0,
					-8
				],
				"size": [
					16,
					16,
					16
				]
			},
			"minecraft:entity_collision": {
				"origin": [
					-8,
					0,
					-8
				],
				"size": [
					16,
					16,
					16
				]
			}
		},
