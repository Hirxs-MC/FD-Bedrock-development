			"e:cycle_changer": {
				"set_block_property": {
					"p:changer": "q.block_property('p:changer')==3?0:q.block_property('p:changer')+1"
				},
				"run_command": {
					"command": [
						"playsound block.lantern.break @p ~~~",
						"particle foxes:texture ~~~"
					]
				}
			}
