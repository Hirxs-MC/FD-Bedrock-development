{
	"format_version": "1.16.100",
	"minecraft:block": {
		"description": {
			"identifier": "fb:prize_corner_sign",
			"is_experimental": false,
			"properties": {
				"fb:block_rotation": [
					2,
					3,
					4,
					5
				],
				"fb:channel": [
					0,
					1,
					2
				]
			}
		},
		"components": {
			"minecraft:destroy_time": 1,
			"minecraft:creative_category": {
				"category": "construction",
				"group": "construction"
			},
			"minecraft:block_light_filter": 0,
			"minecraft:explosion_resistance": 0.1,
			"minecraft:material_instances": {
				"*": {
					"ambient_occlusion": false,
					"face_dimming": false,
					"texture": "pcs_off",
					"render_method": "alpha_test"
				}
			},
			"minecraft:geometry": "geometry.prize_corner_sign",
			"minecraft:aim_collision": {
				"origin": [
					-8,
					0,
					6
				],
				"size": [
					16,
					16,
					2
				]
			},
			"minecraft:block_collision": {
				"origin": [
					-8,
					0,
					6
				],
				"size": [
					16,
					16,
					2
				]
			},
			"minecraft:on_player_placing": {
				"event": "fb:block_placed"
			},
			"minecraft:on_placed": {
				"event": "fb:destroy"
			},
			"minecraft:rotation": [
				0,
				0,
				0
			]
		},
		"events": {
			"fb:destroy": {
				"sequence": [
					{
						"run_command": {
							"target": "self",
							"command": [
								""
							]
						}
					}
				]
			},
			"fb:block_placed": {
				"sequence": [
					{
						"set_block_property": {
							"fb:block_rotation": "q.cardinal_facing_2d"
						}
					},
					{
						"set_block_property": {
							"fb:channel": 0
						}
					}
				]
			},
			"fb:event_off": {
				"sequence": [
					{
						"set_block_property": {
							"fb:channel": 0
						}
					},
					{
						"run_command": {
							"target": "self",
							"command": [
								"gamerule sendcommandfeedback false",
								"playsound random.click @p ~~~ 1 2"
							]
						}
					},
					{
						"run_command": {
							"target": "other",
							"command": [
								"title @s actionbar §eThe sign was turned §7(OFF)"
							]
						}
					}
				]
			},
			"fb:switch_1": {
				"sequence": [
					{
						"condition": "q.is_sneaking",
						"trigger": {
							"event": "fb:event_off",
							"target": "self"
						}
					},
					{
						"condition": "!q.is_sneaking",
						"trigger": {
							"event": "fb:channel_1",
							"target": "self"
						}
					}
				]
			},
			"fb:switch_2": {
				"sequence": [
					{
						"condition": "q.is_sneaking",
						"trigger": {
							"event": "fb:event_off",
							"target": "self"
						}
					},
					{
						"condition": "!q.is_sneaking",
						"trigger": {
							"event": "fb:channel_2",
							"target": "self"
						}
					}
				]
			},
			"fb:off_phone": {
				"sequence": [
					{
						"condition": "q.is_sneaking",
						"trigger": {
							"event": "fb:event_off",
							"target": "self"
						}
					},
					{
						"condition": "!q.is_sneaking",
						"trigger": {
							"event": "fb:channel_0",
							"target": "self"
						}
					}
				]
			},
			"fb:channel_0": {
				"sequence": [
					{
						"set_block_property": {
							"fb:channel": 0
						}
					},
					{
						"run_command": {
							"target": "self",
							"command": [
								"gamerule sendcommandfeedback false",
								"playsound random.click @p ~~~ 1 2"
							]
						}
					},
					{
						"run_command": {
							"target": "other",
							"command": [
								"title @s actionbar §eThe sign was turned §7(OFF)"
							]
						}
					}
				]
			},
			"fb:channel_1": {
				"sequence": [
					{
						"set_block_property": {
							"fb:channel": 1
						}
					},
					{
						"run_command": {
							"target": "self",
							"command": [
								"gamerule sendcommandfeedback false",
								"playsound random.click @p ~~~ 1 2"
							]
						}
					},
					{
						"run_command": {
							"target": "other",
							"command": [
								"title @s actionbar §eArcade Sign on,the pattern is §7(Revolving)"
							]
						}
					}
				]
			},
			"fb:channel_2": {
				"sequence": [
					{
						"set_block_property": {
							"fb:channel": 2
						}
					},
					{
						"run_command": {
							"target": "self",
							"command": [
								"gamerule sendcommandfeedback false",
								"playsound random.click @p ~~~ 1 2"
							]
						}
					},
					{
						"run_command": {
							"target": "other",
							"command": [
								"title @s actionbar §eArcade Sign on,the pattern is §7(Impulse)"
							]
						}
					}
				]
			}
		},
		"permutations": [
			{
				"condition": "q.block_property('fb:block_rotation') == 2",
				"components": {
					"minecraft:rotation": [
						0,
						-180,
						0
					]
				}
			},
			{
				"condition": "q.block_property('fb:block_rotation') == 3",
				"components": {
					"minecraft:rotation": [
						0,
						0,
						0
					]
				}
			},
			{
				"condition": "q.block_property('fb:block_rotation') == 4",
				"components": {
					"minecraft:rotation": [
						0,
						-90,
						0
					]
				}
			},
			{
				"condition": "q.block_property('fb:block_rotation') == 5",
				"components": {
					"minecraft:rotation": [
						0,
						90,
						0
					]
				}
			},
			{
				"condition": "q.block_property('fb:channel') == 0",
				"components": {
					"minecraft:material_instances": {
						"*": {
							"ambient_occlusion": false,
							"face_dimming": false,
							"texture": "ar_off",
							"render_method": "alpha_test"
						}
					},
					"minecraft:on_interact": {
						"event": "fb:switch_1"
					},
					"minecraft:geometry": "geometry.arcade_room_sign",
					"minecraft:on_player_destroyed": {
						"event": "fb:destroy"
					}
				}
			},
			{
				"condition": "q.block_property('fb:channel') == 1",
				"components": {
					"minecraft:block_light_emission": 0.9,
					"minecraft:material_instances": {
						"*": {
							"ambient_occlusion": false,
							"face_dimming": false,
							"texture": "ar_on1",
							"render_method": "alpha_test"
						}
					},
					"minecraft:on_interact": {
						"event": "fb:switch_2"
					},
					"minecraft:geometry": "geometry.arcade_room_sign",
					"minecraft:on_player_destroyed": {
						"event": "fb:destroy"
					}
				}
			},
			{
				"condition": "q.block_property('fb:channel') == 2",
				"components": {
					"minecraft:block_light_emission": 0.9,
					"minecraft:material_instances": {
						"*": {
							"ambient_occlusion": false,
							"face_dimming": false,
							"texture": "ar_on2",
							"render_method": "alpha_test"
						}
					},
					"minecraft:on_interact": {
						"event": "fb:event_off"
					},
					"minecraft:geometry": "geometry.arcade_room_sign",
					"minecraft:on_player_destroyed": {
						"event": "fb:destroy"
					}
				}
			}
		]
	}
}